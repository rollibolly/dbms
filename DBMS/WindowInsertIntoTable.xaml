<Window x:Class="DBMS.WindowInsertIntoTable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DBMS"
        mc:Ignorable="d"
        Title="Insert into table" Height="438.341" Width="530.72">
    <Grid>
        <DataGrid x:Name="dataGridInsertTable" Margin="10,34,10,83" AutoGenerateColumns="False" ItemsSource="{Binding}"/>
        <Button x:Name="btnInsertRows" Content="Insert" Margin="0,0,10,39" Click="btnInsertRows_Click" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75"/>
        <Label x:Name="label1" Content="Values to insert:" HorizontalAlignment="Left" Margin="10,3,0,0" VerticalAlignment="Top"/>
        <StatusBar HorizontalAlignment="Left" Height="29" Margin="0,373,0,0" VerticalAlignment="Top" Width="513">
            <StatusBarItem HorizontalAlignment="Right">
                <TextBlock x:Name="statusText"></TextBlock>
            </StatusBarItem>
        </StatusBar>

    </Grid>
</Window>
